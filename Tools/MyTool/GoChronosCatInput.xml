<tool id="GoChCat_id" name="GoChronosCat" version="0.1.0">
  <!--<requirements>
     <container type="docker">libmesos/ubuntu</container>
  </requirements> -->
  <description>cat an input file in a Mesos Cluster</description>
  <command>cat $input > $output </command>
  <inputs>
    <param format="txt" name="input" type="data" label="Source file"/>
  </inputs>
  <outputs>
    <data format_source="input" name="output" label="${tool.name} on ${on_string}" />
  </outputs>
  <help>
   This tool send a cat file to Mesos via Galaxy.
  </help>

</tool>
